generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model projects {
  id         String @id @default(uuid())
  title      String
  category   String
  image_url  String
  createdAt  DateTime @default(now())
}
